{
  "name": "easy-tsnameof",
  "description": "Typescript nameOf function. Safe types",
  "version": "2.1.0",
  "license": "MIT",
  "author": "Grigoriy Kolenko <kolenko@me.com>",
  "repository": "kolengri/easy-tsnameof",
  "keywords": [
    "typescript",
    "nameOf"
  ],
  "main": "dist/nameOf.js",
  "module": "dist/nameOf.es6.js",
  "typings": "dist/nameOf.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest --env=jsdom --collectCoverage",
    "test:watch": "npm run test -- --watch",
    "start": "cross-env NODE_ENV=development tsc-watch --onSuccess \"rollup -c\"",
    "prebuild": "rimraf dist && npm test",
    "build": "tsc && cross-env NODE_ENV=production rollup -c && rimraf compiled",
    "prepublish": "npm run build",
    "format": "prettier --trailing-comma es5 --single-quote --write 'src/**/*' 'test/**/*'",
    "precommit": "lint-staged",
    "addc": "all-contributors add",
    "gen-docs": "all-contributors generate && doctoc README.md"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.5",
    "all-contributors-cli": "^6.12.0",
    "cross-env": "^7.0.2",
    "doctoc": "^1.3.0",
    "husky": "4.2.5",
    "jest": "^27.4.5",
    "lint-staged": "10.2.2",
    "prettier": "2.5.1",
    "rimraf": "3.0.0",
    "rollup": "1.29.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-filesize": "6.2.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-sourcemaps": "0.5.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-uglify": "6.0.4",
    "ts-essentials": "^6.0.5",
    "ts-jest": "^27.1.2",
    "tsc-watch": "4.6.0",
    "tslib": "^2.3.1",
    "tslint": "^6.1.3",
    "typescript": "latest"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "semi": true
  },
  "dependencies": {
    "ts-toolbelt": "^9.6.0"
  }
}
